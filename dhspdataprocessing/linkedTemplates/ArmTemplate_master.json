{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dhspdataprocessing"
		},
		"ls_datalake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_datalake'"
		},
		"ls_datawarehouse_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_datawarehouse'"
		},
		"ls_raw_data_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_raw_data'"
		},
		"ls_datalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dhspdatalake.dfs.core.windows.net/"
		},
		"ls_datawarehouse_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "thuytd3-sqlserver.database.windows.net"
		},
		"ls_datawarehouse_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "HCMUE"
		},
		"ls_datawarehouse_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "admin1"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dhspdataprocessing_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_datalake_accountKey": {
						"value": "[parameters('ls_datalake_accountKey')]"
					},
					"ls_datawarehouse_password": {
						"value": "[parameters('ls_datawarehouse_password')]"
					},
					"ls_raw_data_connectionString": {
						"value": "[parameters('ls_raw_data_connectionString')]"
					},
					"ls_datalake_properties_typeProperties_url": {
						"value": "[parameters('ls_datalake_properties_typeProperties_url')]"
					},
					"ls_datawarehouse_properties_typeProperties_server": {
						"value": "[parameters('ls_datawarehouse_properties_typeProperties_server')]"
					},
					"ls_datawarehouse_properties_typeProperties_database": {
						"value": "[parameters('ls_datawarehouse_properties_typeProperties_database')]"
					},
					"ls_datawarehouse_properties_typeProperties_userName": {
						"value": "[parameters('ls_datawarehouse_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "dhspdataprocessing_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'dhspdataprocessing_ArmTemplate_0')]"
			]
		}
	]
}